object FormatMe {
  List(number) match {
    case head :: Nil if head % 2 == 0 => "number is even"
    case head :: Nil => "number is not even"
    case Nil => "List is empty"
  }
  function(arg1, arg2(arg3(arg4, arg5, "arg6"), arg7 + arg8), arg9.select(1, 2, 3, 4, 5, 6))
}

class Person(
  name:             String,
  age:              Int            = 24,
  birthdate:        Date,
  astrologicalSign: String         = "libra",
  shoeSize:         Int,
  favoriteColor:    java.awt.Color
)
